{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx server/index.ts",
    "build": "vite build && esbuild server/index.ts --platform=node --bundle --format=esm --external:dotenv --external:@babel/preset-typescript --external:lightningcss --outfile=dist/index.js",
    "start": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "postinstall": "npm install axios dotenv @babel/preset-typescript lightningcss"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "openai": "^4.90.0",
    "zod": "^3.23.8",
    "@babel/preset-typescript": "^7.24.1",
    "lightningcss": "^1.23.0",
    "better-sqlite3": "^11.9.1",
    "ws": "^8.18.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "lucide-react": "^0.453.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-icons": "^5.4.0",
    "react-resizable-panels": "^2.1.4",
    "recharts": "^2.13.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.0",
    "wouter": "^3.3.5"
  },
  "devDependencies": {
    "tsx": "^4.19.1",
    "vite": "^5.4.14",
    "esbuild": "^0.25.0",
    "typescript": "5.6.3",
    "@types/node": "20.16.11",
    "@types/express": "^4.17.21",
    "@vitejs/plugin-react": "^4.3.2"
  }
}
